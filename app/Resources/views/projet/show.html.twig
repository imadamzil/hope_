{% extends 'base.html.twig' %}

{% block title %} Hope3k | Projet {% endblock %}{% block menu %}  <a
        href="{{ path('projet_index') }}">Projet</a>   {% endblock %}
{% block menu_active %}    Détails {% endblock %}{% block content %}


    <div class="row">
        <div class="col-xs-12">
            <!-- PAGE CONTENT BEGINS -->


            <div class="hr dotted"></div>


            <div class="">
                <div id="user-profile-2" class="user-profile">
                    <div class="tabbable">


                        <div class="tab-content no-border padding-24">
                            <div id="home" class="tab-pane active">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-3 center">


                                        <div class="space space-4"></div>

                                        <a href="{{ path('projet_facturation', { 'id': projet.id }) }}" class="btn btn-sm btn-block btn-success">
                                            <i class="ace-icon fa fa-money bigger-120"></i>
                                            <span class="bigger-110">Facturer</span>
                                        </a>
                                        <a href="{{ path('projet_edit', { 'id': projet.id }) }}" class="btn btn-sm btn-block btn-warning">
                                            <i class="ace-icon fa fa-edit bigger-120"></i>
                                            <span class="bigger-110">Modifier</span>
                                        </a>
                                        {% if projet.statut =='Terminé' %}

                                        {% else %}
                                            <a href="{{ path('projet_end', { 'id': projet.id }) }}"
                                               class="btn btn-sm btn-block btn-primary">
                                                <i class="ace-icon fa fa-close bigger-110"></i>
                                                <span class="bigger-110">Marquer comme terminé</span>
                                            </a>
                                        {% endif %}
                                    </div><!-- /.col -->

                                    <div class="col-xs-12 col-sm-9">
                                        <h4 class="blue">
                                            <span class="middle">{{ projet.nom }}</span>
                                            {% if projet.statut == 'Terminé' %}


                                                <span class="label label-danger pull-right">

                                                <i class="ace-icon fa fa-close smaller-80 align-middle"></i>
                                                    {{ projet.statut }}

												</span>


                                            {% else %}
                                                <span class="label label-info pull-right">

                                                <i class="ace-icon fa fa-circle smaller-80 align-middle"></i>
                                                    {{ projet.statut }}

												</span>
                                            {% endif %}
                                        </h4>

                                        <div class="profile-user-info">


                                            <div class="profile-info-row">
                                                <div class="profile-info-name"> BC clinet</div>

                                                <div class="profile-info-value">


                                                    <span>
                                                        {% if projet.bcclient is null %}
                                                            --
                                                        {% else %}
                                                           BC_ {{ projet.bcclient.client.nom }}
                                                        {% endif %}

                                                    </span>
                                                </div>
                                            </div>


                                            <div class="profile-info-row">
                                                <div class="profile-info-name"> Date Début</div>

                                                <div class="profile-info-value">
                                                    <span>{% if projet.dateDebut %}{{ projet.dateDebut|date('Y-m-d') }}{% endif %}</span>
                                                </div>
                                            </div>
                                            <div class="profile-info-row">
                                                <div class="profile-info-name"> Date Fin</div>

                                                <div class="profile-info-value">
                                                    <span>{% if projet.dateFin %}{{ projet.dateFin|date('Y-m-d') }}{% endif %}</span>
                                                </div>
                                            </div>

                                            <div class="profile-info-row">
                                                <div class="profile-info-name"> Date Création</div>

                                                <div class="profile-info-value">
                                                    <span>{% if projet.createdAt %}{{ projet.createdAt|date('Y-m-d') }}{% endif %}</span>
                                                </div>
                                            </div>
                                            <div class="profile-info-row">
                                                <div class="profile-info-name"> Date Modification</div>

                                                <div class="profile-info-value">
                                                    <span>{% if projet.updatedAt %}{{ projet.updatedAt|date('Y-m-d') }}{% endif %}</span>
                                                </div>
                                            </div>
                                        </div>


                                    </div><!-- /.col -->
                                </div><!-- /.row -->

                                <div class="space-20"></div>

                                <div class="col-md-10 col-sm-offset-1">
