                                                    <li class="active">
                                                        <a data-toggle="tab" href="#task-tab" aria-expanded="true">Consultants</a>
                                                    </li>

                                                    <li class="">
                                                        <a data-toggle="tab" href="#member-tab" aria-expanded="false">Factures</a>
                                                    </li>
                                                    <li class="">
                                                        <a data-toggle="tab" href="#bc-tab" aria-expanded="false">Bc fournisseurs</a>
                                                    </li>
                                                    <li class="">
                                                        <a data-toggle="tab" href="#production-tab" aria-expanded="false">Production</a>
                                                    </li>


                                                </ul>
                                            </div>
                                        </div>

                                        <div class="widget-body">
                                            {% if projet.projetconsultants | length == 0 %}

